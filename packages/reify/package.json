{
	"name": "@sylphx/reify",
	"version": "0.1.2",
	"description": "Reify - Reified mutations with pluggable interpreters. Describe once, execute anywhere.",
	"type": "module",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js"
		}
	},
	"files": [
		"dist"
	],
	"scripts": {
		"build": "tsc",
		"prepack": "bun run build",
		"test": "bun test",
		"typecheck": "tsc --noEmit"
	},
	"keywords": [
		"reify",
		"mutation",
		"pipeline",
		"plugin",
		"optimistic-update",
		"type-safe",
		"serializable",
		"effect"
	],
	"author": "Sylphx",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/SylphxAI/reify.git"
	},
	"dependencies": {
		"@sylphx/reify-core": "workspace:*",
		"@sylphx/reify-entity": "workspace:*",
		"@sylphx/reify-adapter-cache": "workspace:*",
		"@sylphx/reify-adapter-prisma": "workspace:*"
	}
}
